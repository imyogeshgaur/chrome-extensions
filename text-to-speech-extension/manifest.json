{
    "manifest_version" : 2,
    "name" : "Text To Speech",
    "version" : "1.0",
    "description" : "Convert any text to Speech",
    "icons" : {
        "128" : "img/icons120.png",
        "48" : "img/icons48.png",
        "16" : "img/icons16.png"
    },
    "browser_action" : {
        "default_icon" : "img/icons16.png",
        "default_popup" : "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}